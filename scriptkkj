MainTab:CreateButton({
	Name = "üåê Foder dinheiro de geral",
	Callback = function()
		Rayfield:Notify({
			Title = "Servidor",
			Content = "Processando jogadores...",
			Duration = 2
		})

		task.wait(2)

		Rayfield:Notify({
			Title = "Conclu√≠do",
			Content = "Processo finalizado",
			Duration = 2
		})

	 local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local TransferRemote =
    ReplicatedStorage
    :WaitForChild("Modules")
    :WaitForChild("Packages")
    :WaitForChild("Knit")
    :WaitForChild("Services")
    :WaitForChild("EconomyService")
    :WaitForChild("RE")
    :WaitForChild("Transfer")

local valor = "9999999999999999999" -- 10 mil
local cooldown = 16   -- 15 segundos

local function notify(titulo, texto)
    StarterGui:SetCore("SendNotification", {
        Title = titulo,
        Text = texto,
        Duration = 5
    })
end

notify("foder dinheiro de geral", "dando dinheiro pra geral aguarde")

for i, player in ipairs(Players:GetPlayers()) do
    local args = {
        player.Name,
        valor
    }

    TransferRemote:FireServer(unpack(args))

    notify(
        "Transfer√™ncia realizada ",
        "Transferido "..valor.." para "..player.Name..
        "\nAguarde "..cooldown.." segundos para o pr√≥ximo..."
    )

    if i < #Players:GetPlayers() then
        task.wait(cooldown)
    end
end

notify("finalizado", "Todos os jogadores receberam o dinheiro dupado")

	end
})

